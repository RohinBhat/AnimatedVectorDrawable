                    android:strokeColor="#000000"
                    android:strokeWidth="1"
                    android:strokeLineJoin="round"/>
            </group>
            <group
                android:name="group_2"
                android:translateX="3"
                android:translateY="3">
                <path
                    android:name="path_2"
